<?xml version="1.0" encoding="utf-8"?>
<manifest package="hgl.ican.io.animationdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".AnimatorApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewAnimator.ViewAnimatorActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>

                <action android:name="hgl.ican.io.action.view_animator"/>
            </intent-filter>
        </activity>
        <activity android:name=".valueAnimator.ValueAnimatorActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>

                <action android:name="hgl.ican.io.action.value_animator"/>
            </intent-filter>
        </activity>
        <activity android:name=".animatorSet.AnimatorSetActivity">
        </activity>
        <activity
            android:name=".drawableAnimator.DrawableAnimatorActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>

                <action android:name="hgl.ican.io.action.drawable_animator"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".materialAnimator.MaterialAnimatorActivity"
            android:theme="@style/AppTheme.AppBarOverlay">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>

                <action android:name="hgl.ican.io.action.material_animator"/>
            </intent-filter>
        </activity>
        <activity android:name=".materialAnimator.TransitionActivity1">
        </activity>
        <activity android:name=".materialAnimator.SharedElementActivity">
        </activity>
        <activity android:name=".materialAnimator.AnimationsActivity1">
        </activity>
        <activity android:name=".materialAnimator.RevealActivity">
        </activity>
    </application>

</manifest>